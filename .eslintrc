{
    "extends": [
        "airbnb-base",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-return-assign": "off",
        "consistent-return": "off",
        "no-use-before-define": "off",
        "no-unused-vars":"off",
        "class-methods-use-this":"off",
        "no-named-as-default-member":"off",
        "no-named-as-default":"off",
        "import/no-extraneous-dependencies":"off",
        "func-names": "off",
        "prettier/prettier": [
            "error"
        ]
    }
}
